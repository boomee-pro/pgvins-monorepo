generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String @id @default(cuid())
  email     String @unique
  firstName String
  lastName  String
  password  String
}

model Wine {
  id     String @id @default(cuid())
  name   String @unique
  origin String
  price  Float
}
